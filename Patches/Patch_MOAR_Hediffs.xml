                                <RangedWeapon_Cooldown>-2</RangedWeapon_Cooldown>
                                <MeleeWeapon_CooldownMultiplier>-0.45</MeleeWeapon_CooldownMultiplier>
                                <ShootingAccuracyPawn>15</ShootingAccuracyPawn>
                                <HuntingStealth>0.5</HuntingStealth>
