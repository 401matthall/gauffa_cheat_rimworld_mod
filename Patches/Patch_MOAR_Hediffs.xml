    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Atlas.AndroidTiers</li>
        </mods>
        <match Class="PatchOperationReplace">
            <xpath>/Defs/HediffDef[defName="PlatingComposite"]</xpath>
            <value>
                <HediffDef ParentName="ImplantHediffBase">
                <defaultLabelColor>(0.6, 0.6, 0.9)</defaultLabelColor>
                    <defName>PlatingComposite</defName>
                    <label>arcotech frame plating</label>
                    <labelNoun>arcotech plating</labelNoun>
                    <description>Installed arcotech frame plating.</description>
                    <initialSeverity>0.05</initialSeverity>
                    <maxSeverity>1</maxSeverity>
                    <comps>
                        <li Class="HediffCompProperties_SeverityPerDay">
                            <severityPerDay>0.05</severityPerDay>
                        </li>
                    </comps>
                    <stages>
                        <li>
                            <minSeverity>0.05</minSeverity>
                            <label>Initializing</label>
                            <statOffsets>
                                <ArmorRating_Sharp>0.4</ArmorRating_Sharp>
                                <ArmorRating_Blunt>0.2</ArmorRating_Blunt>
                                <ComfyTemperatureMax>10</ComfyTemperatureMax>
                                <ComfyTemperatureMin>-10</ComfyTemperatureMin>
                            </statOffsets>
                            <capMods>
                                <li>
                                    <capacity>Moving</capacity>
                                    <offset>-0.2</offset>
                                </li>
                                <li>
                                    <capacity>Manipulation</capacity>
                                    <offset>-0.2</offset>
                                </li>
                            </capMods>
                        </li>
                        <li>
                            <minSeverity>0.25</minSeverity>
                            <label>Calibrating</label>
                            <statOffsets>
                                <ArmorRating_Sharp>1</ArmorRating_Sharp>
                                <ArmorRating_Blunt>1</ArmorRating_Blunt>
                                <ArmorRating_Heat>1</ArmorRating_Heat>
                                <ComfyTemperatureMax>20</ComfyTemperatureMax>
                                <ComfyTemperatureMin>-20</ComfyTemperatureMin>
                            </statOffsets>
                            <capMods>
                                <li>
                                    <capacity>Moving</capacity>
                                    <offset>-0.1</offset>
                                </li>
                                <li>
                                    <capacity>Manipulation</capacity>
                                    <offset>-0.1</offset>
                                </li>
                            </capMods>
                        </li>
                        <li>
                            <minSeverity>0.45</minSeverity>
                            <label>Calibrating</label>
                            <statOffsets>
                                <ArmorRating_Sharp>2</ArmorRating_Sharp>
                                <ArmorRating_Blunt>2</ArmorRating_Blunt>
                                <ArmorRating_Heat>2</ArmorRating_Heat>
                                <ComfyTemperatureMax>30</ComfyTemperatureMax>
                                <ComfyTemperatureMin>-30</ComfyTemperatureMin>
                            </statOffsets>
                        </li>
                        <li>
                            <minSeverity>0.75</minSeverity>
                            <label>Calibrating</label>
                            <statOffsets>
                                <ArmorRating_Sharp>4</ArmorRating_Sharp>
                                <ArmorRating_Blunt>4</ArmorRating_Blunt>
                                <ArmorRating_Heat>4</ArmorRating_Heat>
                                <ComfyTemperatureMax>40</ComfyTemperatureMax>
                                <ComfyTemperatureMin>-40</ComfyTemperatureMin>
                            </statOffsets>
                        </li>
                        <li>
                            <minSeverity>1</minSeverity>
                            <label>Actualized</label>
                            <statOffsets>
                                <ArmorRating_Sharp>5</ArmorRating_Sharp>
                                <ArmorRating_Blunt>5</ArmorRating_Blunt>
                                <ArmorRating_Heat>5</ArmorRating_Heat>
                                <ComfyTemperatureMax>50</ComfyTemperatureMax>
                                <ComfyTemperatureMin>-50</ComfyTemperatureMin>
                            </statOffsets>
                        </li>
                    </stages>
                    <spawnThingOnRemoved>CompositePlating</spawnThingOnRemoved>
                    <addedPartProps>
                        <solid>true</solid>
                    </addedPartProps>
                </HediffDef>
            </value>
        </match>
    </Operation>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Atlas.AndroidTiers</li>
        </mods>
        <match Class="PatchOperationReplace">
            <xpath>/Defs/HediffDef[defName="SoldierMind"]</xpath>
            <value>
                <HediffDef ParentName="ImplantHediffBase">
                    <defaultLabelColor>(0.7, 0.6, 0.7)</defaultLabelColor>
                    <defName>SoldierMind</defName>
                    <label>Soldier upgrade</label>
                    <labelNoun>a soldier upgrade</labelNoun>
