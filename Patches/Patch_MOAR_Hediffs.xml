                                <RangedWeapon_Cooldown>-9</RangedWeapon_Cooldown>
                                <MeleeWeapon_CooldownMultiplier>-0.99</MeleeWeapon_CooldownMultiplier>
                                <ShootingAccuracyPawn>25</ShootingAccuracyPawn>
